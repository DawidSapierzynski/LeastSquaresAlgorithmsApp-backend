<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="addDataSeriesFileTable" author="DS">
        <createTable tableName="data_series_file">
            <column name="data_series_file_id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" primaryKeyName="pk_data_series_file" nullable="false"/>
            </column>
            <column name="user_id" type="bigint">
                <constraints nullable="false" foreignKeyName="fk_user_data_series_file" referencedTableName="user"
                             referencedColumnNames="user_id"/>
            </column>
            <column name="name" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
            <column name="hash_name" type="varchar(200)">
                <constraints nullable="false"/>
            </column>
            <column name="date_sent" type="timestamp">
                <constraints nullable="false"/>
            </column>
            <column name="size" type="int"/>
            <column name="variance" type="double"/>
            <column name="is_deleted" type="boolean" defaultValueBoolean="false">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>