<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="addApproximationPropertiesTable" author="DS">
        <createTable tableName="approximation_properties">
            <column name="approximation_properties_id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" primaryKeyName="pk_approximation_properties" nullable="false"/>
            </column>
            <column name="data_series_file_id" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_series_data_file_series_properties"
                             referencedTableName="data_series_file"
                             referencedColumnNames="data_series_file_id"/>
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_user_approximation_properties"
                             referencedTableName="user"
                             referencedColumnNames="user_id"/>
            </column>
            <column name="degree_approximation" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="date_create" type="timestamp">
                <constraints nullable="false"/>
            </column>
            <column name="is_deleted" type="boolean">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>